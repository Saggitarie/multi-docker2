{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "saggitarie/multi-client2",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "saggitarie/multi-server2",
      "hostname": "server",
      "essential": false
    },
    {
      "name": "worker",
      "image": "saggitarie/multi-worker2",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "saggitarie/multi-nginx2",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
